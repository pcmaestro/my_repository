#Manera sencilla de indicar las propiedades de una clase
class Juguete1():
    nombre = ""
    precio = ""
    
#end juguete

patines = Juguete1()

patines.nombre = "patines"
patines.precio = 0

#Pero la forma habitual es hacerlo con un constructor

class Juguete2():
    def __init__(self):
        self.nombre = ""
        self.precio = 0
        self.cantidad = 12
    
    